{
    "expo": {
        "name": "gym",
        "version": "1.0.0",
        "slug": "gym",
        "sdkVersion": "31.0.0",
        "platforms": ["ios", "android"],
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "assets/**",
            "node_modules/react-navigation/src/**/*.png"
        ],
        "ios": {
            "supportsTablet": true
        }
    }
}
